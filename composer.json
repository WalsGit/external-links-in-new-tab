{
    "name": "walsgit/external-links-in-new-tab",
    "description": "Opens all external links in a new tab",
    "keywords": [
        "flarum", "flarum-extention", "extension", "links", "url", "forum", "nav"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2.0"
    },
    "authors": [
        {
            "name": "Wa!id",
            "homepage": "https://discuss.flarum.org/u/Walid"
        }
    ],
    "autoload": {
        "psr-4": {
            "Walsgit\\ExternalLinksInNewTab\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "External Links In New Tab",
            "category": "feature",
            "icon": {
                "name": "fas fa-external-link-alt",
                "color": "#fff",
                "backgroundColor": "#008e97"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": true,
                "forum": true,
                "js": true,
                "jsCommon": true,
                "css": true,
                "locale": true,
                "gitConf": true,
                "githubActions": true,
                "prettier": true,
                "typescript": true,
                "bundlewatch": false,
                "backendTesting": true,
                "editorConfig": true,
                "styleci": true
            }
        }
    },
    "support": {
        "issues": "https://github.com/WalsGit/external-links-in-new-tab/issues"
    }
}
